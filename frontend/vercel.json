{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://barcoder1.onrender.com/api/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "has": [
        {
          "type": "host",
          "value": "https://barcoder1.vercel.app/"
        }
      ],
      "statusCode": 200
    }
  ]
}
